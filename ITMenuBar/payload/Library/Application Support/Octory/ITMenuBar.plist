                </dict>
				<dict>
					<key>Type</key>
					<string>ExecuteCommand</string>
					<key>Command</key>
					<string>system_profiler SPSoftwareDataType | awk &apos;/System Version/ {print $3, $4, $5}&apos;</string>
					<key>Variable</key>
					<string>OS_VERSION</string>
				</dict>
				<dict>
					<key>Type</key>
					<string>ExecuteCommand</string>
					<key>Command</key>
					<string>uptime | sed -E &apos;s/^[^,]*up *//; s/, *[[:digit:]]* users.*//; s/min/minutes/; s/([[:digit:]]+):0?([[:digit:]]+)/\1 hours, \2 minutes/&apos; | tr -d &quot;\n&quot;</string>
					<key>Variable</key>
					<string>DEVICE_UPTIME</string>
				</dict>
				<dict>
					<key>Type</key>
					<string>ExecuteCommand</string>
					<key>Command</key>
					<string>ifconfig | grep &quot;inet &quot; | head -n 2 | tail -n 1 | cut -d &quot; &quot; -f 2</string>
					<key>Variable</key>
					<string>LOCAL_IP</string>
				</dict>
				<dict>
					<key>Type</key>
					<string>ExecuteCommand</string>
					<key>Command</key>
					<string>dig -4 TXT +short o-o.myaddr.l.google.com @ns1.google.com | tr -d &quot;\042&quot;</string>
					<key>Variable</key>
					<string>REMOTE_IP</string>
				</dict>
